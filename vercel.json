{
    "version": 2,
    "routes": [
      {
        "src": "/api/sets/([0-9]+)",
        "dest": "/api/sets.go"
      },
      {
        "src": "/api/allsets",
        "dest": "/api/allsets.go"
      },
      {
        "src": "/docs/(.*)",
        "dest": "/docs/$1"
      },
      {
        "src": "^/$",
        "dest": "/lego-app/"
      }
    ]
  }
  