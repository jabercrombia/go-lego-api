{
  "builds": [
    {
      "src": "lego-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "lego-go/api/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "lego-go/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "lego-app/$1"
    }
  ]
}
